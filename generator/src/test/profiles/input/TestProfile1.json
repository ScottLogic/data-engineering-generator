{
	"schemaVersion": "v3",
	"description": "DTCC",
	"fields":
	[
		{ "name": "Level" },
		{ "name": "Type" },
		{ "name": "Identifier" },
		{ "name": "Event" }
	],
	"rules": [{
		"rule": "Level",
		"constraints": [{
			"field": "Level",
			"is": "inSet",
			"values": [
				"First",
				"Second",
				"Third",
				"Fourth",
				"Fifth"
			]
		},{
			"not": {
				"field": "Level",
				"is": "null"
			}
		},{
			"if": {
				"allOf": [{
					"field": "Type",
					"is": "inSet",
					"values": [
						"A",
						"B"
					]
				}]
			},
			"then": {
				"allOf": [{
					"field": "Level",
					"is": "inSet",
					"values": [
						"Second",
						"Third"
					]
				}]
			}
		},{
			"if": {
				"allOf": [{
					"field": "Type",
					"is": "inSet",
					"values": [
						"C"
					]
				}]
			},
			"then": {
				"allOf": [{
					"field": "Level",
					"is": "equalTo",
					"value": "First"
				}]
			}
		}]
	},{
		"rule": "Type",
		"constraints": [{
			"field": "Type",
			"is": "inSet",
			"values": [
				"A",
				"B",
				"C",
				"D",
				"E"
			]
		}]
	},{
		"rule": "Identifier",
		"constraints": [{
			"field": "Identifier",
			"is": "inSet",
			"values": [
				"EventA",
				"EventB",
				"EventC"
			]
		}]
	},{
		"rule": "Event",
		"constraints": [{
			"field": "Event",
			"is": "ofType",
			"value": "string"
		},{
			"field": "Event",
			"is": "shorterThan",
			"value": 10
		},{
			"if": {
				"allOf": [{
					"field": "Identifier",
					"is": "equalTo",
					"value": "EventB"
				}]
			},
			"then": {
				"field": "Event",
				"is": "shorterThan",
				"value": 20
			}
		}]
	}]
}