{
	"schemaVersion": "v3",
	"description": "Test 2",
	"fields": 
	[
		{ "name": "Parameter" },
		{ "name": "GraphType" },		
		{ "name": "User" },
		{ "name": "EventType" },
		{ "name": "UserType" }
	],
	"rules": [{
		"rule": "Parameter",
		"constraints": [{
			"field": "Parameter",
			"is": "inSet",
			"values": [
				"A",
				"B",
				"C"
			]
		},{
			"not": {
				"field": "Parameter",
				"is": "null"
			}
		}]
	},{
		"rule": "GraphType",
		"constraints": [{
			"field": "GraphType",
			"is": "inSet",
			"values": [
				"GraphType1",
				"GraphType2",
				"GraphType3",
				"GraphType4"
			]
		},{
			"not": {
				"field": "GraphType",
				"is": "null"
			}
		},{
			"if": {
				"allOf": [{
					"field": "Parameter",
					"is": "equalTo",
					"value": "A"
				}]
			},
			"then": {
				"allOf": [{
					"field": "GraphType",
					"is": "inSet",
					"values": [
						"GraphType2",
						"GraphType4"
					]
				}]
			}
		},{
			"if": {
				"allOf": [{
					"field": "Parameter",
					"is": "equalTo",
					"value": "C"
				}]
			},
			"then": {
				"allOf": [{
					"field": "GraphType",
					"is": "inSet",
					"values": [
						"GraphType2",
						"GraphType3"
					]
				}]
			}
		}]
	},{
		"rule": "User",
		"constraints": [{
			"field": "User",
			"is": "ofType",
			"value": "string"
		},{
			"not": {
				"field": "User",
				"is": "null"
			}
		},{
			"field": "User",
			"is": "shorterThan",
			"value": 50
		},{
			"if": {
				"allOf": [{
					"field": "UserType",
					"is": "equalTo",	
					"value": "UserTypeA"
				}]				
			},
			"then": {
				"allOf": [{
					"field": "User",
					"is": "inSet",
					"values": [
						"Administrator",
						"Moderator",
						"Editor"
					]
				}]
			}
		},{
			"if": {
				"allOf": [{
					"field": "UserType",
					"is": "inSet",
					"values": [
						"UserTypeB",
						"UserTypeC"
					]
				}]
			},
			"then": {
				"allOf": [{
					"field": "User",
					"is": "shorterThan",
					"value": 20
				}]
			}
		}]
	},{
		"rule": "EventType",
		"constraints": [{
			"field": "EventType",
			"is": "ofType",
			"value": "numeric"
		},{
			"field": "EventType",
			"is": "lessThan",
			"value": 40
		},{
			"if": {
				"allOf": [{
					"field": "Parameter",
					"is": "equalTo",
					"value": "B"
				}]
			},
			"then": {
				"allOf": [{
					"field": "EventType",
					"is": "inSet",
					"values": [
						10,
						15,
						20,
						30,
						35
					]
				}]
			}
		}]
	},{
		"rule": "UserType",
		"constraints": [{
			"field": "UserType",
			"is": "inSet",
			"values": [
				"UserTypeA",
				"UserTypeB",
				"UserTypeC",
				"UserTypeD"
			]
		},{
			"not": {
				"field": "UserType",
				"is": "null"
			}
		}]
	}]
}